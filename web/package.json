{
  "name": "moo-web",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "repository": "https://github.com/imf4ll/moo.git",
  "author": "z3oxs <z3ox1s@protonmail.com>",
  "license": "MIT",
  "workspaces": [
    "backend",
    "frontend"
  ],
  "packageManager": "yarn@2.4.3",
  "scripts": {
    "dev": "concurrently \"yarn workspace frontend run dev\"  \"cd backend; go run main.go\"",
    "build": "yarn workspaces foreach --parallel run build",
    "start": "yarn workspaces foreach --parallel run start"
  },
  "dependencies": {
    "concurrently": "^8.0.1"
  }
}
